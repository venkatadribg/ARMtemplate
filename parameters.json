        "publicIpAddressName": {
            "value": "ARMVM-ip"
        },
        "publicIpAddressType": {
            "value": "Static"
        },
        "publicIpAddressSku": {
            "value": "Standard"
        },
        "pipDeleteOption": {
            "value": "Detach"
        },
        "virtualMachineName": {
            "value": "ARMVM"
        },
        "virtualMachineComputerName": {
            "value": "ARMVM"
        },
        "virtualMachineRG": {
            "value": "Demo"
        },
        "osDiskType": {
            "value": "Premium_LRS"
        },
        "osDiskDeleteOption": {
            "value": "Delete"
        },
        "virtualMachineSize": {
            "value": "Standard_DS1_v2"
        },
        "nicDeleteOption": {
            "value": "Detach"
        },
        "adminUsername": {
            "value": "venkatadmin"
        },
        "adminPassword": {
            "value": null
        },
        "patchMode": {
            "value": "AutomaticByOS"
        },
        "enableHotpatching": {
            "value": false
        }
    }
}